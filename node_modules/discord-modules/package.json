{
    "name": "discord-modules",
    "version": "4.0.0",
    "description": "Discord.js framework used to implement a modular Discord bot.",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "format": "prettier --write .",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "build": "tsc && tsc-alias",
        "release": "standard-version"
    },
    "repository": {
        "type": "git",
        "url": "git@gitlab.com:unhcs/discord/discord-modules/discord-modules.git"
    },
    "keywords": [
        "discordjs",
        "discord",
        "discord-modules",
        "types"
    ],
    "author": {
        "name": "Jason Waleryszak",
        "email": "contributor@jasonwaleryszak.com",
        "url": "https://jasonwaleryszak.com/"
    },
    "license": "MIT",
    "devDependencies": {
        "@trivago/prettier-plugin-sort-imports": "^3.2.0",
        "@tsconfig/node18": "^1.0.0",
        "@types/node": "^17.0.36",
        "@typescript-eslint/eslint-plugin": "^5.27.0",
        "@typescript-eslint/parser": "^5.27.0",
        "@zerollup/ts-transform-paths": "^1.7.18",
        "eslint": "^8.16.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-typescript": "^2.7.1",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import": "^2.26.0",
        "prettier": "2.6.2",
        "standard-version": "^9.5.0",
        "tsc-alias": "^1.6.9",
        "typescript": "^4.7.2"
    },
    "dependencies": {
        "@discordjs/builders": "^0.13.0",
        "@discordjs/rest": "^0.4.1",
        "discord-api-types": "^0.33.1",
        "discord.js": "^13.7.0",
        "jsonschema": "^1.4.1"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,json}": "eslint --cache --fix",
        "*.{js,jsx,ts,tsx,json,md}": "prettier --write"
